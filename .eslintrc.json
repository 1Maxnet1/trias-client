{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"env": {
		"node": true
	},
	"plugins": [
		"@typescript-eslint",
		"jest"
	],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:jest/recommended",
		"prettier"
	],
	"rules": {
		"@typescript-eslint/no-unused-vars": "off"
	},
	"ignorePatterns": [
		"lib"
	]
}
